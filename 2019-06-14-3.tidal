d1 $ sound "bass3" |+| n "~ 1 [~1] [~1]" # speed 0.5 

d3 $ jux rev $  sound "bass3" |+| n "~ 1 [~3] [~3]" # speed 0.5 # gain (slow 16 sine)

d5 $ sound (fast 2 "[~ bass3] bass3" ) # speed 0.5 # vowel "o e"

d7 $ off 0.25 (# vowel "o o e") $ sound "bd" |+| n (fast (choose [0.5, 0.5, 2])"~ 8 20 [~20]") # speed 0.5 # gain 0.9
