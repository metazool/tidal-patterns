d1 $ sound "~ jungbass:4 ~ ~" # speed 0.5

d2 $ sound "~ [~ jungbass] ~ jungbass?" # speed 0.5

d3 $ fast 2 (sound "~ [~ jungbass?] ~ jungbass") # speed 0.8

d4 $ someCycles (# vowel "e") $ sound "~ [~ hh?] ~ [~ hh]" 

d5 $ jux rev $ sound "~ [jungle:4 ~] ~ ~" # legato 0.5 # speed (choose [0.5, 0.2]) 

d6 $ sound "jungle:6" # gain (slow 16 sine) # speed 0.5 

d7 $ sound "[~ pebbles?]" # gain 0.5 # legato 4 
